# https://stackoverflow.com/a/49661967
<IfModule mod_rewrite.c>
    RewriteEngine On

    ####################
    # Local version:
    RewriteBase /
    ####################

    ####################
    # BTC version:
    RewriteBase /t/btc/
    ####################

    ####################
    # BCB version:
    # RewriteBase /t/bcb/
    ####################
    
    
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule (.*) index.html [QSA,L]
</IfModule>


